{
    "name": "ns147/sodium-auth",
    "description": "auth module for laravel",
    "keywords": [
        "auth",
        "authentication",
        "laravel"
    ],
    "homepage": "https://github.com/diegosal/sodium-auth",
    "support": {
        "issues": "https://github.com/diegosal/sodium-auth/issues",
        "source": "https://github.com/diegosal/sodium-auth"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Diego Gonzalez",
            "email": "diego.sal.gzz@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.2",
        "paragonie/paseto": "^1.0.1"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Ns147\\SodiumAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ns147\\SodiumAuth\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ns147\\SodiumAuth\\Providers\\SodiumAuthServiceProvider"
            ]
        }
    }
}