{
    "name": "ns147/sodium-auth",
    "description": "auth module for laravel",
    "keywords": [
        "auth",
        "authentication",
        "laravel"
    ],
    "homepage": "https://github.com/diegosal/sodium-auth",
    "support": {
        "issues": "https://github.com/diegosal/sodium-auth/issues",
        "source": "https://github.com/diegosal/sodium-auth"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Diego Gonzalez",
            "email": "diego.sal.gzz@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.2",
        "illuminate/auth": "5.6.*",
        "illuminate/contracts": "5.6.*",
        "illuminate/http": "5.6.*",
        "illuminate/support": "5.6.*",
        "paragonie/sapient": "1.*",
        "paragonie/halite": "4.*"
    },
    "require-dev": {
        "cartalyst/sentinel": "2.0.*",
        "illuminate/console": "5.6.*",
        "illuminate/database": "5.6.*",
        "illuminate/routing": "5.6.*",
        "mockery/mockery": ">=1.1.0",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Ns147\\SodiumAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ns147\\SodiumAuth\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ns147\\SodiumAuth\\Providers\\SodiumAuthServiceProvider"
            ]
        }
    }
}